            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "pictureURL": {
            "type": "string",
            "nullable": true
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "BasketItemDTO": {
        "required": [
          "brand",
          "category",
          "id",
          "pictureURL",
          "price",
          "productName",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "productName": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "minimum": 0.1,
            "type": "number",
            "format": "double"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "pictureURL": {
            "minLength": 1,
            "type": "string"
          },
          "brand": {
            "minLength": 1,
            "type": "string"
          },
          "category": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Brand": {
        "type": "object",
